#include<stdio.h>
#include <stdlib.h>
struct process{
	int priority;
	int burst_time;
	int pid;
	int waiting_time;
	int turnaround_time;
	int remaining_time;
	int arrival_time;
};
void getInput();
void calcWaitingTime(struct process *q,int);
void calcTurnAroundTime(struct process *q,int);
void printQueue(struct process *q,int size);
void RoundRobin();
void PrioSorting();
void FCFS();
void printQueueI(struct process);
void printQueue(struct process *,int);
